{
  "servers": {
    "utility": {
      "name": "Utility MCP Server", 
      "version": "1.0.0",
      "description": "A server that provides utility functions",
      "directory": "./servers/utility",
      "enabled": true,
      "entryFile": "index.js"
    },
    "firecrawl": {
      "name": "Firecrawl MCP Server",
      "version": "1.3.2",
      "description": "Web scraping and crawling server using Firecrawl API",
      "directory": "./servers/firecrawl",
      "enabled": true,
      "entryFile": "index.js",
      "apiKeyParam": "FIRECRAWL_API_KEY"
    },
    "tavily": {
      "name": "Tavily MCP Server",
      "version": "0.2.9",
      "description": "AI-powered web search, extraction, crawling and mapping using Tavily API",
      "directory": "./servers/tavily",
      "enabled": true,
      "entryFile": "index.js",
      "apiKeyParam": "TAVILY_API_KEY"
    }
  },
  "global": {
    "jwt_secret": "your-super-secret-jwt-key",
    "port": 8080,
    "session_cleanup_interval": 300000,
    "log_file": "multi-mcp-debug.log",
    "enable_dns_rebinding_protection": false,
    "supported_protocol_versions": ["2025-06-18", "2024-11-05"]
  }
}